package com.announcify.engine.c2dm;

import org.scribe.model.Token;

import com.announcify.engine.model.Credential;

public class C2DMCredential extends Credential {
    
    public static C2DMCredential getCredentialForIdentity(String userId) {
        return (C2DMCredential) Credential.getCredentialForIdentity(C2DMCredential.class, userId);
    }
    
    
    String regId;
    
    
    public C2DMCredential() {
        // TODO Auto-generated constructor stub
    }
    
    public C2DMCredential(Token token, String userId, String regId) {
        super(token, userId);
        
        this.regId = regId;
    }


    public String getRegId() {
        return regId;
    }
}
