package com.announcify.api;

import android.content.Context;

import com.announcify.api.background.util.PluginSettings;

public class SimpleAnnouncifyIntent extends AnnouncifyIntent {

    public SimpleAnnouncifyIntent(Context context) {
        super(context, null);
    }

    private class SimpleSettings extends PluginSettings {

        public SimpleSettings(Context context) {
            super(context, "");
        }

        @Override
        public String getEventType() {
            return null;
        }

        @Override
        public int getPriority() {
            return 9;
        }
    }
}
